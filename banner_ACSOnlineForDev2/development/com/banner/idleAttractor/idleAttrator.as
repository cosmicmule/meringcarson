package  com.banner.idleAttractor{		import flash.display.Sprite;			// custom classes	import com.greensock.*; 	import com.greensock.easing.*;	import com.banner.util.DynamicGradientText;		public class idleAttrator extends Sprite {				private var stageRef:Object;		private var parentRef:Object;				private var container:Sprite = new Sprite();		private var dynamic_text_segements:Array = new Array("GET","READY","TO","THINK","FAST!");				private var objLibrary = [];		private var wordOffset:Number = 30;				public function idleAttrator(Appstage:Object,reference:Object) {			// constructor code			stageRef = Appstage;			parentRef = reference;			create_Objects();			add_to_stage();						container.x = (parentRef.stageWidth)/2 - container.width/2;			container.y = 15;						addChild(container);		}				private function create_Objects(obj:Array = null){			var localObj:Array;						// object override			if(obj != null){localObj = obj}else{localObj = dynamic_text_segements}						for(var e=0; e < localObj.length;e++){				var tempTextField:DynamicGradientText = new DynamicGradientText(localObj[e],60)								tempTextField. name = e;				objLibrary.push(tempTextField)			}					}				private function add_to_stage(){			for(var e= 0; e < objLibrary.length;e++){								var pos_Placement = 10;								if(e > 0){					pos_Placement += (objLibrary[e - 1].width + objLibrary[e - 1].x) - wordOffset;					objLibrary[e].x = pos_Placement;				}else{					objLibrary[e].x = pos_Placement;				}								container.addChild(objLibrary[e]);				TweenMax.to(container, 0, {glowFilter:{color:0x000000, alpha:1, blurX:10, blurY:10}});							}		}												// END Constructor	}	}